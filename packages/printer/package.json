{
  "name": "@dprint/rust-printer",
  "version": "0.1.0",
  "description": "Wasm bindings for printing with rust.",
  "main": "dist/dprint-rust-printer.js",
  "types": "lib/dprint-rust-printer.d.ts",
  "author": "David Sherret",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsherret/dprint/issues"
  },
  "homepage": "https://github.com/dsherret/dprint#readme",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsherret/dprint.git"
  },
  "scripts": {
    "build": "rimraf dist && yarn build:rust && yarn rollup && yarn build:declarations",
    "build:web": "rimraf dist && yarn build:rust-web && yarn rollup && yarn build:declarations",
    "build:rust": "wasm-pack build --release --target nodejs --out-dir src-ts/pkg",
    "build:rust-web": "wasm-pack build --release --target web --out-dir src-ts/pkg",
    "build:rust-debug": "wasm-pack build --dev --target nodejs --out-dir src-ts/pkg",
    "build:declarations": "ts-node --project scripts/tsconfig.json --transpile-only scripts/buildDeclarations.ts",
    "rollup": "copyfiles -u 1 src-ts/pkg/**/* dist && rollup --config",
    "test": "yarn build:rust && cross-env TS_NODE_COMPILER=\"ttypescript\" TS_NODE_TRANSPILE_ONLY=\"true\" mocha",
    "test:debug": "yarn test --inspect-brk"
  },
  "dependencies": {
    "@dprint/types": "~0.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "@types/ts-nameof": "^4.2.1",
    "chai": "^4.2.0",
    "copyfiles": "^2.2.0",
    "cross-env": "^6.0.3",
    "mocha": "^7.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-morph": "^6.0.2",
    "ts-nameof": "^4.2.2",
    "ts-node": "^8.6.2",
    "ttypescript": "^1.5.10"
  }
}
